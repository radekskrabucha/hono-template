    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.11.17",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "globals": "15.11.0",
    "prettier": "3.3.3",
    "tsx": "4.7.1",
    "typescript-eslint": "8.8.1"
